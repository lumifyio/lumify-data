<div class="diffs-list">
<table class="table table-striped">
  <thead>
    <tr>
      <th>Message</th>
      <th>Details</th>
      <th><button class="btn btn-mini">Publish All</button></th>
    </tr>
  </thead>
  <tbody>
    <% diffs.forEach(function(diff) { %>
    <tr>
      <td><%= diff.message %></td>
      <td><% 

        switch(diff.type) {
          case 'PropertyDiffItem':
            if (diff.old && diff.new) { %>  
              <%= formatValue(diff.name, diff.new) %> 
              <div style="text-decoration:line-through"><%= formatValue(diff.name, diff.old) %></div>  
            <% } else if (diff.new) { %> 
              <%= formatValue(diff.name, diff.new) %> 
            <% } 
            break;
        }
      %></td>
      <td class="actions"><button class="btn btn-mini">Publish</button></td>
    </tr>
    <% }); %>
  </tbody>
</table>
</div>
