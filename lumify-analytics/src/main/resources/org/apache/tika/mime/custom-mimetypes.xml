<?xml version="1.0" encoding="UTF-8"?>
<mime-info>
    <mime-type type="application/wikipedia.page+xml">
        <glob pattern="*.xml"/>
        <magic priority="100">
            <match value="&lt;page" type="string" offset="0:8192">
                <match value='xmlns="http://www.mediawiki.org/xml/export' type="string" offset="0:8192"/>
            </match>
        </magic>
        <sub-class-of type="application/xml" />
    </mime-type>
</mime-info>
