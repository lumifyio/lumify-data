
<li class="nav-header">Workspaces</li>

<% results.forEach(function(result) { %>
<li class="<%= selected === result.rowKey ? 'active' : '' %>" 
    data-rowkey="<%= result.rowKey %>"
    ><a href="#"><button class="delete btn btn-mini btn-danger">Delete</button><span><%= result.title || result.rowKey %></span></a></li>
<% }); %>

<li>
    <input class="new" name="title" type="text" placeholder="Workspace name" />
    <button class="new btn btn-mini">Add</button>
</li>

