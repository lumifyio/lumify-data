<li class="<%= selected === workspace._rowKey ? 'active' : '' %>" 
    data-users="<%= JSON.stringify(workspace.permissions || []) %>"
    data-created-by="<%= workspace.createdBy || '' %>"
    data-is-editable="<%= workspace.isEditable || false %>"
    data-title="<%= workspace.title %>"
    data-_row-key="<%= workspace._rowKey %>"
    ><a>
      <span class="badge" style="display:none"/><button class="disclosure btn btn-mini"></button><span><%= workspace.title || workspace._rowKey %></span></a></li>
