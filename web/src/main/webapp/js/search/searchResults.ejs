<% results.forEach(function(result) { %>
<li data-info="<%= JSON.stringify({
      'rowKey': result.rowKey,
      'type': type,
      'subType': subType
    }) %>"><a href="<%= result.url %>"><%= result.title || "No title available" %><div><span class="date"><%= result.publishedDate ? new Date(result.publishedDate).toDateString() : '' %></span><span class="source"><%= result.source || '' %></span></div></a>
</li>
<% }); %>
