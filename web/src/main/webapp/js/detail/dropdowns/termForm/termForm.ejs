<div class="form">
  <div class="text sign"><%= sign %></div>
  <select>
    <option>Loading...</option>
  </select>
  <input 
    data-provide="typeahead" 
    autocomplete="off" 
    placeholder="Associate with Object..." 
    class="object-sign" 
    type="text" 
    value="<%= objectSign || sign %>"></input>

  <div class="buttons">
    <button class="create-term btn btn-small btn-primary"><%= buttonText %></button>
  </div>
</div>
