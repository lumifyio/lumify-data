<% properties.forEach(function(property) { %>
    <li class="property property-<%= property.key %>">
      <strong><%= property.displayName %></strong><span class="value"><%= property.value %></span>
    </li>
<% }); %>

  <li class="buttons">
    <button class="add-new-properties btn btn-mini btn-default">Add Property</button>
  </li>

