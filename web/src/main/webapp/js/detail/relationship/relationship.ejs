<div class="btn-toolbar">
  <%- highlightButton %>
</div>

<h2>
<% var data = JSON.stringify({
      id : sourceId,
      title : sourceTitle,
      type : sourceType,
      rowKey : sourceRowKey,
      subType : sourceSubType}); %>

<span class="node-to-node-relationship" data-info='<%- data %>' >
    <a href="javascript:" class="vertex">
        <%= sourceTitle %>
    </a> ->
</span>

<% var data = JSON.stringify({
      id : targetId,
      title : targetTitle,
      type : targetType,
      rowKey : targetRowKey,
      subType : targetSubType}); %>

<span class="node-to-node-relationship" data-info='<%- data %>'>
    <a href="javascript:" class="vertex" >
        <%= targetTitle %>
    </a>
</span>
</h2>
<h3>Properties</h3>
<div class="properties">
  <% properties.forEach(function(property) {%>
        <li>
        <%= property.key%>: <%= property.value%>
        </li>
      <%}); %>
</div>
