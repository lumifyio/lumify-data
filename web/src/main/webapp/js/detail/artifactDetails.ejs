<h2><%= artifact.Generic_Metadata.subject %></h2>
<% if(artifact.Dynamic_Metadata && artifact.Dynamic_Metadata.latitude) { %>
    <b>Location:</b>
    <a class="map-coordinates" latitude="<%= artifact.Dynamic_Metadata.latitude %>" longitude="<%= artifact.Dynamic_Metadata.longitude %>">
        <%= artifact.Dynamic_Metadata.latitude %>, <%= artifact.Dynamic_Metadata.longitude %>
    </a>
    <br/>
<% } %>
<a href="<%= artifact.rawUrl %>" target="_blank">view</a>
<a href="<%= artifact.rawUrl %>?download=true" target="_blank">download</a>
<div class="text"><%- artifact.contentHtml %></div>